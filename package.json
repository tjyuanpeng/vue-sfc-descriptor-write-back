{
  "name": "vue-sfc-descriptor-write-back",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.23.0",
  "description": "writeback vue SFCDescriptor to sourcecode",
  "author": "tjyuanpeng <tjyuanpeng@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/tjyuanpeng/vue-sfc-descriptor-write-back",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjyuanpeng/vue-sfc-descriptor-write-back.git"
  },
  "bugs": {
    "url": "https://github.com/tjyuanpeng/vue-sfc-descriptor-write-back/issues"
  },
  "keywords": [
    "vue",
    "sfc",
    "descriptor",
    "toString",
    "parse",
    "stringify",
    "writeback"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start": "tsdown --watch",
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.5.26",
    "magic-string": "^0.30.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@types/node": "^25.0.6",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.2.0",
    "tsdown": "0.20.0-beta.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
